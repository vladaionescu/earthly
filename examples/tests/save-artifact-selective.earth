VERSION --referenced-save-only 0.5

savea:
    FROM alpine:latest
    RUN echo apple > a
    SAVE ARTIFACT a AS LOCAL a

saveb:
    FROM alpine:latest
    RUN echo bannana > b
    SAVE ARTIFACT b AS LOCAL b

savec:
    FROM alpine:latest
    RUN echo cherry > c
    SAVE ARTIFACT c AS LOCAL c

saved:
    FROM +savec
    RUN echo durian > d
    SAVE ARTIFACT d AS LOCAL d

savee:
    FROM +saved
    RUN echo elderberry > e
    SAVE ARTIFACT e AS LOCAL e

test1:
    BUILD +savea
    BUILD +saveb

test2:
    BUILD +savec

test3:
    BUILD +saved

test4sub:
    BUILD +savee

test4:
    BUILD +test4sub

all:
    BUILD +test1
    BUILD +test2
    BUILD +test3
    BUILD +test4
